{
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/dist",
  "installCommand": "echo 'Skipping root install'",
  "framework": "vite",
  "env": {
    "NODE_ENV": "production",
    "VITE_API_URL": "https://mysteryverse-backend-production.up.railway.app"
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}